    Font.Name = 'Courier New'
    Font.Style = []
    Lines.Strings = (
      'ALTER TABLE SMSHistorico ADD Usuario int null'
      'GO'
      'ALTER TABLE Membros ADD Email varchar(100) null'
      'GO'
      'ALTER TABLE MembrosLog ADD Email varchar(100) null'
      'GO'
      
        'IF  EXISTS (SELECT * FROM sys.triggers WHERE object_id = OBJECT_' +
        'ID(N'#39'[dbo].[AcoesMembros]'#39'))'
      'DROP TRIGGER [dbo].[AcoesMembros]'
      'GO'
      'CREATE TRIGGER AcoesMembros'
      '   ON Membros'
      '   FOR UPDATE, DELETE'
      'AS'
      'DECLARE @Tipo CHAR(1)'
      
        '  IF (SELECT COUNT(*) FROM inserted) > 0 AND (SELECT COUNT(*) FR' +
        'OM deleted) > 0'
      '    SET @Tipo = '#39'A'#39
      '  ELSE'
      '    SET @Tipo = '#39'E'#39
      'INSERT INTO MembrosLog'
      '('
      'DataLog, Tipo, FichaNumero, Nome, Sexo,'
      
        'CartaoMembro, DataNascimento, DataValidade, DataBatismo, MembroD' +
        'esde,'
      
        'Naturalidade, EstadoCivil, NomeConjuge, FiliacaoPai, FiliacaoMae' +
        ', Escolaridade,'
      
        'Profissao, RG, CPF, EndRua, EndNumero, EndBairro, EndComplemento' +
        ', EndCidade,'
      'EndEstado, EndCep, FoneResid, FoneCelular, Status, Email'
      ')'
      '('
      'SELECT'
      'GETDATE(), @Tipo, FichaNumero, Nome, Sexo,'
      
        'CartaoMembro, DataNascimento, DataValidade, DataBatismo, MembroD' +
        'esde,'
      
        'Naturalidade, EstadoCivil, NomeConjuge, FiliacaoPai, FiliacaoMae' +
        ', Escolaridade,'
      
        'Profissao, RG, CPF, EndRua, EndNumero, EndBairro, EndComplemento' +
        ', EndCidade,'
      'EndEstado, EndCep, FoneResid, FoneCelular, Status, Email'
      'FROM deleted'
      ')'
      'GO'
      'ALTER TABLE Membros ALTER COLUMN MembroDesde DateTime null'
      'GO'
